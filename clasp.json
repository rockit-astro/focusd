{
  "daemon": "clasp_focus",
  "log_name": "focusd@clasp",
  "control_machines": ["CLASPTCS"],
  "serial_port": "/dev/focuser",
  "serial_baud": 9600,
  "serial_timeout": 5,
  "channels": 2,
  "idle_loop_delay": 5,
  "moving_loop_delay": 0.5,
  "move_timeout": 180,
  "temperature_probes": {
    "rasa1": {
      "label": "RASA1",
      "address": "2834A2540A000023"
    },
    "rasa2": {
      "label": "RASA2",
      "address": "28AE071E0B0000EC"
    }
  }
}
